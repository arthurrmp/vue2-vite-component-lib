    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^1.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^23.0.0",
    "prettier": "^2.0.0",
    "typescript": "~4.7.4",
    "vite": "^5.0.11",
    "vitest": "^1.0.0",
    "vue-template-compiler": "^2.7.16",
